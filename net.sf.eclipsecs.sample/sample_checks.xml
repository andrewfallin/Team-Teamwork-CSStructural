<?xml version="1.0" encoding="UTF-8"?>
<!-- This configuration file was written by the eclipse-cs plugin configuration editor -->
<!-- Checkstyle-Configuration: Test Description: none -->
<!DOCTYPE module PUBLIC "-//Checkstyle//DTD Check Configuration 1.3//EN" "https://checkstyle.org/dtds/configuration_1_3.dtd">
<module name="Checker">
    <property name="severity" value="warning" />

    <module name="Header">
        <property name="headerFile" value="${config_loc}/java.header" />
    </module>

    <module name="TreeWalker">

        <module name="MethodLimit">
            <property name="max" value="1" />
            <message key="methodlimit" value="Man, only {0} methods allowed." />
        </module>
        
        <module name="HalsteadChecks">
            <property name="operatorCount" value="0" />
            <message key="operatorcount" value="NA" />
            <property name="operandCount" value="0" />
            <message key="operandCount" value="NA" />
            <property name="uniqueoperatorcount" value="0" />
            <message key="uniqueOperatorCount" value="NA" />
            <property name="uniqueoperandcount" value="0" />
            <message key="uniqueOperandCount" value="NA" />
            <property name="halsteadLength" value="0" />
            <message key="halsteadlength" value="NA" />
            <property name="halsteadVocabulary" value="0" />
            <message key="halsteadvocab" value="NA" />
            <property name="halsteadVolume" value="0" />
            <message key="halsteadvolume" value="NA" />
            <property name="halsteadDifficulty" value="0" />
            <message key="halsteaddifficulty" value="NA" />
            <property name="halsteadEffort" value="0" />
            <message key="halsteadeffort" value="NA" />
        </module>
        
        <module name="SuperCheck">
            <property name="countLocals" value="0" />
            <message key="localcount" value="NA" />
            <property name="countExternals" value="0" />
            <message key="externalcount" value="NA" />
            <property name="CommentCount" value="0" />
			<message key="commentcount" value="NA" />
            <property name="CommentLinesCount" value="0" />
            <message key="commentlinescount" value="NA" />
            <property name="VariableCount" value="0" />
			<message key="variablecount" value="NA" />
            <property name="LoopCount" value="0" />
            <message key="loopscount" value="NA" />
            <property name="ExpCount" value="0" />
            <message key="numberofexpressions" value="NA" />
        </module>
        
        <!-- <module name="LineLength"> <property name="max" value="${maxLineLength}"/> </module> -->

        <module name="Regexp">
            <property name="format"
                value="(interface [a-zA-Z0-9 &lt;&gt;,\./*?[\s]]* \{)|((@Immutable|@ThreadSafe|@NotThreadSafe)(.*[\n])*.*(class|enum) [a-zA-Z0-9\s&lt;&gt;,\./*?]* \{)" />
            <property name="message" value="Classes should be annotated with @Immutable, @ThreadSafe or @NotThreadSafe" />
            <property name="illegalPattern" value="false" />
        </module>
    </module>
</module>
